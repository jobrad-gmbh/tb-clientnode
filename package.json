{
  "name": "clientnode",
  "version": "1.0.127",
  "description": "upgrade to object orientated rock solid plugins",
  "documentationWebsite": {
    "name": "clientNode",
    "tagline": "upgrade to object orientated rock solid plugins<!--deDE:Schreibe Objekt orientierte solide Plugins-->",
    "trackingCode": "UA-40192634-7",
    "languages": [
      "deDE",
      "enUS"
    ]
  },
  "keywords": [
    "client",
    "tools",
    "node",
    "polymorph",
    "jquery"
  ],
  "files": [
    "index.js",
    "test.js",
    "test.compiled.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/clientNode"
  },
  "homepage": "http://torben.website/clientNode",
  "main": "index.compiled.js",
  "webOptimizer": {
    "injection": {
      "external": {
        "aliases": {
          "buffer/index.js": "Object",
          "jquery": "jQuery"
        }
      },
      "internal": {
        "vendor": {
          "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['buildDLL', 'serve', 'testInBrowser', 'watchDLL'].includes(self.givenCommandLineArguments[2]) ? self.givenCommandLineArguments[2] === 'serve' ? [] : ['script!qunitjs', 'weboptimizer/browserAPI'] : []"
        }
      }
    },
    "module": {
      "aliases": {
        "clientnode$": "./index",
        "jquery$": "jquery/dist/jquery.min"
      },
      "optimizer": {
        "uglifyJS": null
      },
      "preprocessor": {
        "javaScript": {
          "configuration": {
            "presets": {
              "__evaluate__": "(self.target === 'node' ? ['es2017-node7'] : [['modern-browsers', {loose: true}], 'es2017']).concat('stage-0')"
            }
          }
        }
      },
      "skipParseRegularExpressions": {
        "__evaluate__": "/\\/(?:(?:jquery)|(?:module)|([^/]+\\.min))\\.js(?:\\?.+|$)/"
      }
    }
  },
  "scripts": {
    "buildTest": "babel --plugins transform-flow-strip-types,transform-runtime --presets modern-browsers,es2017,stage-0 --source-maps inline --out-file test.compiled.js test.js",
    "build": "webOptimizer build && npm run buildTest",
    "buildDLL": "webOptimizer buildDLL",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "export": "webOptimizer build '{development:{tool:false},module:{preprocessor:{javaScript:{configuration:{plugins:{__remove__:\"transform-runtime\"}}}}}}' && npm run buildTest",
    "exportIE10Compatible": "webOptimizer build '{development:{tool:false},module:{preprocessor:{javaScript:{configuration:{plugins:{__remove__:\"transform-runtime\",__append__:\"transform-proto-to-assign\"}}}}}}' && npm run buildTest",
    "lint": "webOptimizer typeCheck && webOptimizer lint",
    "prepublish": "if ! npm run build; then echo You should rerun install procedure to resolve dependency cycle. && babel --plugins transform-flow-strip-types,transform-runtime --presets modern-browsers,es2017,stage-0 --out-file index.compiled.js index.js; fi",
    "serve": "webOptimizer serve",
    "start": "npm run serve",
    "test": "webOptimizer test",
    "testInBrowser": "webOptimizer testInBrowser",
    "update": "npm version patch && npm publish",
    "updateDocumentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watchDLL": "webOptimizer buildDLL --watch"
  },
  "license": "CC-BY-3.0",
  "devDependencies": {
    "babel-plugin-transform-proto-to-assign": "latest",
    "documentation-website": "latest",
    "rimraf": "latest",
    "jsdom": "latest",
    "jquery": "latest",
    "qunitjs": "latest",
    "qunit-cli": "latest",
    "source-map-support": "latest",
    "weboptimizer": "latest"
  },
  "peerDependencies": {
    "jquery": "*"
  }
}
