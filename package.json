{
    "name": "tools",
    "version": "1.0.0",
    "description": "upgrade to object orientated rock solid plugins",
    "keywords": ["tools"],
    "files": ["index.js"],
    "repository": {
        "type": "git", "url": "https://github.com/thaibault/tools"
    },
    "homepage": "http://torben.website/tools",
    "main": "index.compiled.js",
    "webOptimizer": {
        "injection": {"internal": {"vendor": {
            "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['buildDLL', 'testInBrowser', 'watchDLL'].includes(self.givenCommandLineArguments[2]) ? ['qunitjs', 'webOptimizer/browserAPI'] : []"
        }}},
        "module": {"skipParseRegularExpression": {
            "__evaluate__": "/\\/(?:(?:jquery)|(?:module)|([^/]+\\.min))\\.js(?:\\?.+|$)/"
        }}
    },
    "scripts": {
        "build": "webOptimizer build",
        "buildDLL": "webOptimizer buildDLL",
        "clear": "webOptimizer clear",
        "document": "webOptimizer document",
        "lint": "webOptimizer typeCheck && webOptimizer lint",
        "postinstall_back": "webOptimizer build || babel --plugins transform-flow-strip-types --presets latest,stage-0 --source-maps inline --out-file index.compiled.js index.js",
        "postinstall": "babel --plugins transform-flow-strip-types --presets latest,stage-0 --source-maps inline --out-file index.compiled.js index.js",
        "preinstall_back": "webOptimizer preinstall || echo You should run you install procedure again to resolve dependency cycles.",
        "serve": "webOptimizer serve",
        "start": "npm run serve",
        "test": "webOptimizer test",
        "testInBrowser": "webOptimizer testInBrowser",
        "watch": "webOptimizer build --watch",
        "watchDLL": "webOptimizer buildDLL --watch"
    },
    "license": "CC-BY-3.0",
    "dependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git"
    },
    "devDependencies": {
        "jsdom": ">=9.4.1",
        "jquery": ">=3.1.0",
        "qunitjs": ">=2.0.0",
        "qunit-cli": ">=0.2.0*",
        "source-map-support": ">=0.4.2"
    },
    "peerDependencies": {"jquery": "*"}
}
